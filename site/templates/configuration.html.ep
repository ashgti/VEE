<!DOCTYPE html>
<html>
    <head>
    	<title>SEE</title>
    	<script src="/js/jquery.min.js" type="text/javascript"></script>
    	<script src="/js/arduinolator.js" type="text/javascript"></script>
    	<link rel="stylesheet" href="/styles/screen.css" type="text/css" media="screen" />
    </head>
    <body>
        <script id="io_template" type="text/x-jquery-tmpl"> 
            <fieldset>
                <legend>${id}</legend>
                <p>Signal Id: <span>${id}</span></p>
                <p>Value: <span>${val}</span></p>
                <p>I/O: <span>${ivo}</span></p>
            </fieldset>
        </script>

        <script id="mapping_template" type="text/x-jquery-tmpl"> 
            <fieldset>
                <legend>Mapping id: ${id}</legend>
                <p>Correlation:</p>
                <p>Timing</p>
                <p>Ratio</p>
                <fieldset>
                    <legend>I/O Events</legend>
                    ${content}
                </fieldset>
            </fieldset>
        </script>

        <script id="event_type_pin" type="text/x-jquery-tmpl">
            
        </script>

        <script id="event_type_serial" type="text/x-jquery-tmpl">
            <label for="timing">Timing:</label>
            <select name="timing" id="timing">
                <option selected value="det">Determinate</option>
            </select>
            <label for="correlation">Output Expected:</label>
            <input type="text" name="foo" value="Hello World" id="foo">
            <label for="ratio">Ratio:</label>
            <input type="text" name="ratio" value="1/5" id="ratio">
        </script>

        <nav>
            <a href="/run_test">Run another Test</a>
        </nav>
        <header>
            <h1>Test Results</h1>
        </header>
        <!-- <a id="config_pin" href="#">Configure Input/Output Pin</a> -->
        <div id="step_zero">
            <label for="simulation_length">Simulation Length:</label>
            <br>
            <input type="text" name="simulation_length" value="" id="simulation_length">
            <br>
            <label>Events List:</label>
            <br>
            <select multiple name="events_list" id="events_list"></select>
            <div id="info">
            </div>
            <div id=buttons>
                <a id=remove href="#">Remove</a>
            </div>
        </div>
        <hr>
        <div id="step_one">
            <!-- <form id="one">
                <fieldset>
                    <legend>I/O Signal COunt</legend>
                    <label for="input_count">Input</label>
                    <input name="input_count" id="input_count">
                    <br>
                    <label for="output_count">Output</label>
                    <input name="output_count" id="output_count">
                    <br>
                    <input id="update_io_count" type="submit" value="Update">
                </fieldset>
            </form> -->
        </div>
        <hr>
        <div id="step_two">
            <form id="two">
                <fieldset>
                    <legend>Configure Events</legend>
                    <fieldset>
                        <legend>I/O Event:</legend>
                        <label for="id">Signal ID:</label>
                        <br>
                        <input name="id" id="id" value="">
                        <br>
                        <!-- <label>Event Type</label>
                        <br>
                        <select name="e_type" id="e_type">
                            <option selected value="digital">Digital</option>
                            <option value="serial">Serial</option>
                        </select> -->
                        <br>
                        <label>Signal Type</label>
                        <br>
                        <select name="e_type">
                            <option>Deterministic</option>
                            <option>Uniform</option>
                            <option>Exponential</option>
                        </select>
                        <br>
                        <label for=val>Initial Val:</label>
                        <br>
                        <input name=val id="val" value="">
                        <br>
                        <!-- 
                        # det signal, initial value, length, ratio, process
                        # uni signal, initial value, a min, b max, a min / 2, process
                        # exp signal, initial value, average, average/s, process
                        -->
                        <label for=len>Length:</label>
                        <br>
                        <input name=length id=length>
                        <br>
                        <label for=ratio>Ratio:</label>
                        <br>
                        <input name=ratio id=ratio>
                        <br>
                        <label for=process>Processing Length:</label>
                        <br>
                        <input name=process id=process>
                        <br>
                        <a href=# id=add_event>Save I/O Event</a>
                    </fieldset>
                    <br>
                </fieldset>
            </form>
        </div>

        <div id="wrapper">
            <div id="signals">
                <h2>Signals</h2>
                <table>
                    <tr>
                        <td>
                            <canvas id="canvas" width="400" height="400"></canvas><br />
                        </td>
                        <td id="feedback">
                        </td>
                    </tr>
                </table>
                <span style="width: 400px; text-align: center;">Time (s)</span>
            </div>
            <nav>
                <a href="/run_test">Run another Test</a>
            </nav>
            <div id="settings">
                <h2>Pin Settings</h2>
                <form>
                    <!--
                    <p>
                        <label for="default_configuration">Default Configuration</label>
                        <select id="default_configuration" name="default_configuration">
                            <option value="other">Other</option>
                            <option selected value="mega">Mega</option>
                            <option value="duem">Duemilanove</option>
                            <option value="nano">Nano</option>
                        </select>
                    </p>
                    <p>
                        <label for="default">Default</label>
                        <input type="checkbox" name="a" value="" id="a">
                    </p>
                    <p>
                        <label for="">Digital Pins</label>
                        <input name="d_pin_count" value="54" />
                        <br />
                        <label for="">Analog Pins</label>
                        <input name="a_pin_count" value="16" />
                    </p>
                    -->
                    <p>
                        <textarea style="width: 300px; height: 300px" id="output_sample" name="output_sample">
length: 20.5
identifiers: ab
---
det a, 1, 2, 2, 4
det b, 1, 3, 2, 4
</textarea>
                    </p>
                </form>
            </div>
            <div id="debug">
                <p id="mouse_pos">Mouse</p>
                <p id="mouse_down">Mouse</p>
                <p id="frames">Time</p>
            </div>
        </div>
    </body>
</html>
